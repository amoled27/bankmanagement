{{#each result}}
<div class="container">
    <div class="card card-body mb-2 mt-2">
        <h4>{{customer_Name}}</h4>
        <p>
            <b>Account ID:</b>{{ account_ID }}</p>
           
        <p>
            <b>Account type:</b><span class="actype">{{acctype}}</span></p>
          
        <p>
            <b>Balance:</b>{{balance}}</p>
    </div>
</div>
{{else}}
<h4>No Customers found</h4>
{{/each}}

